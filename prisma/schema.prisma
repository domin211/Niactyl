// Prisma schema for Niactyl using PostgreSQL

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id         Int    @id @default(autoincrement())
  discord_id String @unique
  username   String
  email      String
  ptero_id   Int?
}

model Egg {
  id          Int    @id
  uuid        String
  name        String
  description String?
  docker_image String
  startup     String
}

model Node {
  id          Int    @id
  name        String
  location_id Int
}
